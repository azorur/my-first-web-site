package chap09_inherit;

import animal.Bird;
import animal.Tiger;

public class _02_Overriding {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Tiger tiger = new Tiger();
		tiger.eat();
		tiger.sleep();
		
		Bird bird = new Bird();
		bird.eat();
		bird.sleep();
	}

}
